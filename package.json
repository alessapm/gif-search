{
  "name": "stash-web-challenge",
  "version": "1.0.0",
  "description": "A search engine for the Giphy GIF library. Find everything from funny GIFs, reaction GIFs, unique GIFs and more!",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next dev",
    "lint": "next lint",
    "test": "jest"
  },
  "author": "Alessa Messineo",
  "dependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.10",
    "axios": "^1.6.2",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/axios": "^0.14.0",
    "@types/node": "^20",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "babel-jest",
        {
          "configFile": "./babel.config.js"
        }
      ]
    },
    "automock": false,
    "moduleFileExtensions": [
      "json",
      "js",
      "jsx",
      "node",
      "ts",
      "tsx"
    ],
    "setupFiles": [
      "dotenv/config"
    ],
    "watchman": false
  }
}
